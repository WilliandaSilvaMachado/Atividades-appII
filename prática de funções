 void main () {

 
   var num1 = 3;
   var num2 = 4;
   
   var calculo = (num1 + num2);
   var calculo2 = (num1 * num2 * num2);
   var calculo3 = (num1 * num1);
   var calculo4 = (calculo * 6);
   var calculo5 = (calculo * 3);
   var calculo6 = (num1 | 0 |);
   
   imprimir (calculo);
   imprimir (calculo2);
   imprimir (calculo 3);
   imprimir (calculo 4);
   imprimir (calculo 5);
 }
 
 
 
