 void main () {

 
  numero int = 3;
  
  
    para (int i = 2; i <numero; i ++) {
        if (numero% i == 0) {
      print ("Valor lógico Verdadeiro"); 
   } senão {
      imprimir ("Falso");
   } pausa;
   }
 }

